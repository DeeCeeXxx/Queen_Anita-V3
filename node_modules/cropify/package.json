{
  "name": "cropify",
  "version": "1.0.9",
  "description": "Crop and round image corners seamlessly.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "deploy": "npm publish",
    "deploy:beta": "npm publish --tag beta",
    "test:m": "cd tests && node index.mjs",
    "test:c": "cd tests && node index.js"
  },
  "keywords": [
    "crop-image",
    "canvas-crop",
    "cropify",
    "unburn",
    "upscale"
  ],
  "author": "flameface",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.28",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unburn/cropify.git"
  },
  "bugs": {
    "url": "https://github.com/unburn/cropify/issues"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.51"
  }
}