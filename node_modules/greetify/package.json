{
  "name": "greetify",
  "version": "2.0.1",
  "description": "Futuristic welcome card canvas library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "fonts"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "deploy": "npm publish",
    "deploy:beta": "npm publish --tag beta",
    "test:m": "cd tests && node index.mjs",
    "test:c": "cd tests && node index.js"
  },
  "keywords": [
    "welcome-card",
    "welcome-canvas",
    "canvas",
    "greetify",
    "greeting",
    "discord-canvas",
    "discord.js"
  ],
  "author": "flameface",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/node": "^20.11.28",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unburn/greetify.git"
  },
  "bugs": {
    "url": "https://github.com/unburn/greetify/issues"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.51",
    "cropify": "latest"
  }
}